        success: function (response) {
          $('#loader').hide();
          if (response['code'] == '0') {
            $('#MaterialDetail').html(response['message']);
            $('#MaterialDetailBox').modal({ show: true });
          }
          else if (response['code'] == '1') {
            $('#HomeOurSponsors').html(response['detail']);

            $(function () {
              "use-strict";
              $(".side-nav-right").sideNav({ edge: "right", closeOnClick: !1 }),
                $(".slider").slider({ full_width: !0, slideSpeed: 1000 }),
                $("#owl-testimonial").owlCarousel({ slideSpeed: 600, paginationSpeed: 800, singleItem: !0 }),
                $(".latest-news-owl").owlCarousel({ autoPlay: !1, singleItem: !0 }),
                $("#fakeLoader").fakeLoader({ zIndex: 999, spinner: "spinner1", bgColor: "#ffffff" })
            });
          }
        },
		error: function (jqXHR, exception) {
          $('#loader').hide();
           $('#MaterialDetail').html(jqXHR.status+";I "+jqXHR.responseText+"; "+exception);
          $('#MaterialDetailBox').modal({ show: true });
        }
       /* error: function () {
          $('#loader').hide();
          $('#MaterialDetail').html("You are not connected to internet. Please try again.");
          $('#MaterialDetailBox').modal({ show: true });
        }*/
      });
    }
    function FunMake() {
      var emailid = document.getElementById('make_email');
      if ($('#make_name').val() == '') {
        $('#MaterialDetail').html("Please Enter Name");
        $('#MaterialDetailBox').modal({ show: true });
        $('#make_name').focus();
        return false;
      }
      if ($('#make_email').val() == '') {
        $('#MaterialDetail').html("Please Enter Email");
        $('#MaterialDetailBox').modal({ show: true });
        $('#make_email').focus();
        return false;
      }
      if ($('#make_email').val() != '' && validateEmail(emailid) == false) {
        $('#MaterialDetail').html("Please Enter Valid Email Address");
        $('#MaterialDetailBox').modal({ show: true });
        $('#make_email').focus();
        return false;
      }
      if ($('#make_phone').val() == '') {
        $('#MaterialDetail').html("Please Enter Phone");
        $('#MaterialDetailBox').modal({ show: true });
        $('#make_phone').focus();
        return false;
      }
      if ($('#make_subject').val() == '') {
        $('#MaterialDetail').html("Please Enter Subject");
        $('#MaterialDetailBox').modal({ show: true });
        $('#make_subject').focus();
        return false;
      }
      if ($('#make_message').val() == '') {
        $('#MaterialDetail').html("Please Enter Message");
        $('#MaterialDetailBox').modal({ show: true });
        $('#make_message').focus();
        return false;
      }
      $('#loader').show();
      $.ajax({
        type: "GET",
        url: 'http://socjoi.com/api/join-now-api.php',
        crossDomain: true,
        data: { account_id: window.localStorage["Socjoi_AccountId"], account_code: window.localStorage["Socjoi_AccountCode"], name: $('#make_name').val(), phone: $('#make_phone').val(), email: $('#make_email').val(), subject: $('#make_subject').val(), message: $('#make_message').val() },
        dataType: 'json',
        success: function (response) {
          $('#loader').hide();
          if (response['code'] == '0') {
            $('#MaterialDetail').html(response['message']);
            $('#MaterialDetailBox').modal({ show: true });
          }
          if (response['code'] == '1') {
            $('#make_success').html('Thank you for your interest to be a Volunteer. We will contact you very soon.');
            setTimeout(function () { $('#login-modal').modal('hide'); $('#make_success').html(''); }, 2000);

          }
          $('#make_name').val('');
          $('#make_email').val('');
          $('#make_subject').val('');
          $('#make_phone').val('');
          $('#make_message').val('');

        },
		error: function (jqXHR, exception) {
          $('#loader').hide();
           $('#MaterialDetail').html(jqXHR.status+";J "+jqXHR.responseText+"; "+exception);
          $('#MaterialDetailBox').modal({ show: true });
        }
       /* error: function () {
          $('#loader').hide();
          $('#MaterialDetail').html("You are not connected to internet. Please try again.");
          $('#MaterialDetailBox').modal({ show: true });
        }*/
      });
    }
    function FunContactUs() {
      var emailid = document.getElementById('contact_email');
      var mobile_number = $('#contact_number').val();
      if ($('#contact_name').val() == '') {
        $('#MaterialDetail').html("Please Enter Name");
        $('#MaterialDetailBox').modal({ show: true });
        $('#contact_name').focus();
        return false;
      }
      if (mobile_number.length != 10 || !(/^\d+$/.test(mobile_number))) {
        $('#MaterialDetail').html("Please Enter Valid Contact Number");
        $('#MaterialDetailBox').modal({ show: true });
        $('#contact_number').focus();
        return false;
      }
      if ($('#contact_email').val() == '') {
        $('#MaterialDetail').html("Please Enter Email");
        $('#MaterialDetailBox').modal({ show: true });
        $('#contact_email').focus();
        return false;
      }
      if ($('#contact_email').val() != '' && validateEmail(emailid) == false) {
        $('#MaterialDetail').html("Please Enter Valid Email Address");
        $('#MaterialDetailBox').modal({ show: true });
        $('#contact_email').focus();
        return false;
      }
      if ($('#contact_subject').val() == '') {
        $('#MaterialDetail').html("Please Enter Subject");
        $('#MaterialDetailBox').modal({ show: true });
        $('#contact_subject').focus();
        return false;
      }
      if ($('#contact_message').val() == '') {
        $('#MaterialDetail').html("Please Enter Message");
        $('#MaterialDetailBox').modal({ show: true });
        $('#contact_message').focus();
        return false;
      }
      $('#loader').show();
      $.ajax({
        type: "GET",
        url: 'http://socjoi.com/api/contact-up-api.php',
        crossDomain: true,
        data: { account_id: window.localStorage["Socjoi_AccountId"], account_code: window.localStorage["Socjoi_AccountCode"], name: $('#contact_name').val(), mobile_number: mobile_number, email: $('#contact_email').val(), subject: $('#contact_subject').val(), message: $('#contact_message').val() },
        dataType: 'json',
        success: function (response) {
          $('#loader').hide();
          if (response['code'] == '0') {
            $('#MaterialDetail').html(response['message']);
            $('#MaterialDetailBox').modal({ show: true });
          }
          if (response['code'] == '1') {
            $('html, body').animate({
              scrollTop: $("#hp_contactus_success").offset().top
            }, 1000);

            $('#contactus_success').html('Thank you for contacting us. Our representative will call you very soon.');
            $('#contact_name').val('');
            $('#contact_email').val('');
            $('#contact_subject').val('');
            $('#contact_message').val('');
            setTimeout(function () { $('#contactus_success').html(''); }, 2000);

          }

        },
		error: function (jqXHR, exception) {
          $('#loader').hide();
           $('#MaterialDetail').html(jqXHR.status+";K "+jqXHR.responseText+"; "+exception);
          $('#MaterialDetailBox').modal({ show: true });
        }
        /*error: function () {
          $('#loader').hide();
          $('#MaterialDetail').html("You are not connected to internet. Please try again.");
          $('#MaterialDetailBox').modal({ show: true });
        }*/
      });
    }
    function validateEmail(email) {
      var re_mail = /^([a-zA-Z0-9_\.\-])+\@(([a-zA-Z0-9\-])+\.)+([a-zA-Z])+$/;
      if (!re_mail.test(email.value)) {
        return false;
      }
      return true;
    }
    (function ($) {
      "use strict";
      jQuery(window).scroll(function () {
        if (jQuery(this).scrollTop() > 1) {
          jQuery('.dmtop').css({ bottom: "25px" });
        } else {
          jQuery('.dmtop').css({ bottom: "-100px" });
        }
      });
      jQuery('.dmtop').click(function () {
        jQuery('html, body').animate({ scrollTop: '0px' }, 800);
        return false;
      });

    })(jQuery);
  </script>
