    function FunHomeOurSponsors() {
      $('#loader').show();
      $.ajax({
        type: "GET",
        url: 'http://socjoi.com/api/home-our-sponsors-api.php',
        crossDomain: true,
        data: { account_id: window.localStorage["Socjoi_AccountId"], account_code: window.localStorage["Socjoi_AccountCode"] },
