    function FunHomeOurTeam() {
      $('#loader').show();
      $.ajax({
        type: "GET",
        url: 'http://socjoi.com/api/home-ourteam-api.php',
        crossDomain: true,
        data: { account_id: window.localStorage["Socjoi_AccountId"], account_code: window.localStorage["Socjoi_AccountCode"] },
        dataType: 'json',
        success: function (response) {
          $('#loader').hide();
          if (response['code'] == '0') {
            $('#MaterialDetail').html(response['message']);
            $('#MaterialDetailBox').modal({ show: true });
          }
          else if (response['code'] == '1') {
            $('#HomeOurTeam').html(response['detail']);
          }
          else if (response['code'] == '3') {
            $('#MainHomeOurTeam').hide();
          }
        },
		error: function (jqXHR, exception) {
          $('#loader').hide();
           $('#MaterialDetail').html(jqXHR.status+";G "+jqXHR.responseText+"; "+exception);
          $('#MaterialDetailBox').modal({ show: true });
        }
       /* error: function () {
          $('#loader').hide();
          $('#MaterialDetail').html("You are not connected to internet. Please try again.");
          $('#MaterialDetailBox').modal({ show: true });
        }*/
      });

    }
