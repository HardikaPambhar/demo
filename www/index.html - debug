          $('#MaterialDetailBox').modal({ show: true });
        }*/
      });

    }
    function FunHomePageSlider() {
      $('#loader').show();
      $.ajax({
        type: "GET",
        url: 'http://socjoi.com/api/home-slider-api.php',
        crossDomain: true,
        data: { account_id: window.localStorage["Socjoi_AccountId"], account_code: window.localStorage["Socjoi_AccountCode"] },
        dataType: 'json',
        success: function (response) {
          $('#loader').hide();
          if (response['code'] == '0') {
            $('#MaterialDetail').html(response['message']);
            $('#MaterialDetailBox').modal({ show: true });
          }
          if (response['code'] == '1') {
            $('#HomePageSlider').html(response['detail']);

            $(function () { "use-strict"; $(".side-nav-right").sideNav({ edge: "right", closeOnClick: !1 }), $(".slider").slider({ full_width: !0, slideSpeed: 1000 }), $("#owl-testimonial").owlCarousel({ slideSpeed: 600, paginationSpeed: 800, singleItem: !0 }), $(".latest-news-owl").owlCarousel({ autoPlay: !1, singleItem: !0 }), $("#fakeLoader").fakeLoader({ zIndex: 999, spinner: "spinner1", bgColor: "#ffffff" }) });
          }
        },
		error: function (jqXHR, exception) {
          $('#loader').hide();
           $('#MaterialDetail').html(jqXHR.status+";E "+jqXHR.responseText+"; "+exception);
          $('#MaterialDetailBox').modal({ show: true });
        }
       /* error: function () {
          $('#loader').hide();
          $('#MaterialDetail').html("You are not connected to internet. Please try again.");
          $('#MaterialDetailBox').modal({ show: true });
        }*/
      });

    }
