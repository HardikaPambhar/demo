  <script>
	
    function CheckLogin() {
      if (isNaN(parseInt(window.localStorage["Socjoi_AccountId"]))) {
        document.location = 'login.html';
		
      }
    }
    function GoToTeamDetail(id) {
      window.localStorage["Socjoi_TeamId"] = id;
      location.href = 'team-detail.html';
    }
    function GoToGalleryDetail(id) {
      window.localStorage["Socjoi_GalleryId"] = id;
      location.href = 'gallery-detail.html';
    }

    $(document).ready(function () {
      if(localStorage.Socjoi_Allow_Login == 'yes'){
        $('.login-btn').show();
      }
      FunGetDefaultCss();
     FunSocjoiMenu();
      FunSocjoiLoginModal();
      FunHomePageSlider();
      FunHomePageAboutUs();
      FunHomeOurTeam();
      FunHomeSuccessStories();
     FunHomeOurSponsors();
      FunFooterSocialMedia();

      if (window.localStorage["Socjoi_LOGO"] == '') {
        $('#DivSocjoiLogo').html('<img src="img/logo.png"  height="40">');
      }
      else {
        $('#DivSocjoiLogo').html('<img src="' + window.localStorage["Socjoi_LOGO"] + '" height="40">');
      }

    });

    function FunSocjoiMenu() {
      $('#loader').show();
      $.ajax({
        type: "GET",
        url: 'http://socjoi.com/api/menu-api.php',
        crossDomain: true,
        data: { account_id: window.localStorage["Socjoi_AccountId"], account_code: window.localStorage["Socjoi_AccountCode"], isLogged: window.localStorage["SOCJOI_LOGIN_ID"],member_id:localStorage.Socjoi_Family_Mem_ID  },
        dataType: 'json',
        cache: false,
        success: function (response) {
          $('#loader').hide();
          if (response['code'] == '0') {
            $('#MaterialDetail').html(response['message']);
            $('#MaterialDetailBox').modal({ show: true });
          }
          if (response['code'] == '1') {
            $('.SocjoiMenu').html(response['detail']);
          }
        },
		error: function (jqXHR, exception) {
          $('#loader').hide();
          $('#MaterialDetail').html(jqXHR.status+";A "+jqXHR.responseText+"; "+exception);
          $('#MaterialDetailBox').modal({ show: true });
        }
       /* error: function () {
          $('#loader').hide();
          $('#MaterialDetail').html("You are not connected to internet. Please try again.");
          $('#MaterialDetailBox').modal({ show: true });
        }*/
      });
    }
    function FunSocjoiLoginModal() {
      $('#loader').show();
      $.ajax({
        type: "GET",
        url: 'http://socjoi.com/api/login-modal-api.php',
        crossDomain: true,
        data: { account_id: window.localStorage["Socjoi_AccountId"], account_code: window.localStorage["Socjoi_AccountCode"], isLogged: window.localStorage["SOCJOI_LOGIN_ID"] },
        dataType: 'json',
        cache: false,
        success: function (response) {
          $('#loader').hide();
          if (response['code'] == '0') {
            $('#MaterialDetail').html(response['message']);
            $('#MaterialDetailBox').modal({ show: true });
          }
          if (response['code'] == '1') {
            $("#includedContent").html(response['detail']);
          }
        },
		error: function (jqXHR, exception) {
          $('#loader').hide();
           $('#MaterialDetail').html(jqXHR.status+";B "+jqXHR.responseText+"; "+exception);
          $('#MaterialDetailBox').modal({ show: true });
        }
       /* error: function () {
          $('#loader').hide();
          $('#MaterialDetail').html("You are not connected to internet. Please try again.");
          $('#MaterialDetailBox').modal({ show: true });
        }*/
      });
    }
    function FunGetDefaultCss() {
      $('#loader').show();
      $.ajax({
        type: "GET",
        url: 'http://socjoi.com/api/default-css-api.php',
        crossDomain: true,
        data: { account_id: window.localStorage["Socjoi_AccountId"], account_code: window.localStorage["Socjoi_AccountCode"] },
        dataType: 'json',
        success: function (response) {
          $('#loader').hide();
          if (response['code'] == '0') {
            $('#MaterialDetail').html(response['message']);
            $('#MaterialDetailBox').modal({ show: true });
          }
          if (response['code'] == '1') {
            $('#PutDefaultCss').html(response['detail']);
          }
        },
		error: function (jqXHR, exception) {
          $('#loader').hide();
          $('#MaterialDetail').html(jqXHR.status+";C "+jqXHR.responseText+"; "+exception);
          $('#MaterialDetailBox').modal({ show: true });
        }
        /*error: function () {
          $('#loader').hide();
          $('#MaterialDetail').html("You are not connected to internet. Please try again.");
          $('#MaterialDetailBox').modal({ show: true });
        }*/
      });
    }
    function FunFooterSocialMedia() {
      $('#loader').show();
      $.ajax({
        type: "GET",
        url: 'http://socjoi.com/api/social-media-api.php',
        crossDomain: true,
        data: { account_id: window.localStorage["Socjoi_AccountId"], account_code: window.localStorage["Socjoi_AccountCode"] },
